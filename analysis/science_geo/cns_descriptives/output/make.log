. program athr_loc
  1.     syntax, data(str) samp(str)  wt_var(str)
  2.     local suf = cond("`wt_var'" == "cite_affl_wt", "_wt", "") 
  3.     use ../external/cleaned_samps/cleaned_last5yrs_`data'_`samp', clear
  4.     gen msa_world = msatitle 
  5.     replace msa_world = city if country != "United States"
  6.     gen msa_comb_world = msa_comb
  7.     replace msa_comb_world = city if country != "United States"
  8.     foreach loc in country msa_comb_world inst {
  9.         qui gunique pmid 
 10.         local articles = r(unique)
 11.         qui sum `wt_var'
 12.         local total = round(r(sum))
 13.         assert `total' == `articles'
 14.         qui sum `wt_var' if !mi(`loc') 
 15.         local denom = r(sum) 
 16.         preserve
 17.         if inlist("`loc'", "us_state", "area", "msatitle", "msa_comb") {
 18.             qui keep if country == "United States"
 19.         }
 20.         gcollapse (sum) `wt_var', by(`loc')
 21.         qui hashsort -`wt_var' 
 22.         qui gen perc = `wt_var' / `total' * 100
 23.         li if mi(`loc')
 24.         qui drop if mi(`loc')
 25.         qui gen cum_perc = sum(perc) 
 26.         qui count
 27.         local rank_end = min(r(N),20) 
 28.         li `loc' perc in 1/`rank_end'
 29.         di "Total articles: `total'"
 30.         mkmat perc cum_perc in 1/`rank_end', mat(top_`loc'_`samp'`suf')
 31.         mat top_`loc'_`data'_`samp' = nullmat(top_`loc'_`data'_`samp') , (
> top_`loc'_`samp'`suf')
 32.         qui levelsof `loc' in 1/2
 33.         global top2_`loc'_`data' "`r(levels)'"
 34.         if inlist("`loc'", "inst", "city_full", "msatitle","msa_comb", "ms
> aworld", "msa_comb_world") {
 35.             qui levelsof `loc' in 1/`rank_end'
 36.             global `loc'_`data' "`r(levels)'"
 37.         }
 38.         qui gen rank_grp = "first" if _n == 1
 39.         replace `loc' = "harvard university" if `loc' == "university harva
> rd"
 40.         replace `loc' = "stanford university" if `loc' == "university stan
> ford"
 41.         qui levelsof `loc' if _n == 1 
 42.         global `loc'_first "`r(levels)'"
 43.         qui levelsof `loc' if _n == 2
 44.         global `loc'_second "`r(levels)'"
 45.         qui replace rank_grp = "second" if _n == 2
 46.         qui replace rank_grp = "china" if `loc' == "China"
 47.         qui replace rank_grp = "rest of top 10" if inrange(_n,3,10) & rank
> _grp!="china"
 48.         qui replace rank_grp = "remaining" if mi(rank_grp)
 49.         keep `loc' rank_grp
 50.         qui save ../temp/`loc'_rank_`data'_`samp'`suf', replace
 51.         restore
 52.     }
 53. end

