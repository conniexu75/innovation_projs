. program athr_loc
  1.     syntax, data(str) samp(str)  wt_var(str)
  2.     local suf = ""
  3.     if "`wt_var'" == "cite_affl_wt" local suf "_wt"
  4.     if "`wt_var'" == "impact_affl_wt" local suf "_if"
  5.     if "`wt_var'" == "impact_cite_affl_wt" local suf "_if_wt"
  6.     if "`wt_var'" == "pat_adj_wt" local suf "_pat"
  7.     use ../external/openalex/cleaned_last5yrs_`data'_`samp', clear
  8.     local end 20
  9.     foreach loc in country msa_c_world inst {
 10.         if "`loc'" == "inst" {
 11.             local end 50
 12.         }
 13.         qui gunique pmid 
 14.         local articles = r(unique)
 15.         qui sum `wt_var'
 16.         local total = round(r(sum))
 17.         assert `total' == `articles'
 18.         qui sum `wt_var' if !mi(`loc') 
 19.         local denom = r(sum) 
 20.         preserve
 21.         if inlist("`loc'", "us_state", "area", "msatitle", "msa_comb") {
 22.             qui keep if country == "United States"
 23.         }
 24.         collapse (sum) `wt_var', by(`loc')
 25.         qui hashsort -`wt_var' 
 26.         qui gen perc = `wt_var' / `total' * 100
 27.         li if mi(`loc')
 28.         qui drop if mi(`loc')
 29.         qui gen cum_perc = sum(perc) 
 30.         gen rank = _n 
 31.         save ../temp/rankings_`loc'`suf', replace
 32.         drop rank
 33.         qui count
 34.         local rank_end = min(r(N),`end') 
 35.         li `loc' perc in 1/`rank_end'
 36.         di "Total articles: `total'"
 37.         mkmat perc cum_perc in 1/`rank_end', mat(top_`loc'_`samp'`suf')
 38.         mat top_`loc'_`data'_`samp' = nullmat(top_`loc'_`data'_`samp') , (
> top_`loc'_`samp'`suf')
 39.         qui levelsof `loc' in 1/2
 40.         global top2_`loc'_`data' "`r(levels)'"
 41.         if inlist("`loc'", "inst", "city_full", "msatitle","msa_comb", "ms
> aworld", "msa_c_world") {
 42.             qui levelsof `loc' in 1/`rank_end'
 43.             global `loc'_`data' "`r(levels)'"
 44.         }
 45.         qui gen rank_grp = "first" if _n == 1
 46.         replace `loc' = "harvard university" if `loc' == "university harva
> rd"
 47.         replace `loc' = "stanford university" if `loc' == "university stan
> ford"
 48.         qui levelsof `loc' if _n == 1 
 49.         global `loc'_first "`r(levels)'"
 50.         qui levelsof `loc' if _n == 2
 51. 
.         global `loc'_second "`r(levels)'"
 52.         qui replace rank_grp = "second" if _n == 2
 53.         qui replace rank_grp = "china" if `loc' == "China"
 54.         qui replace rank_grp = "uk" if `loc' == "United Kingdom"
 55.         qui replace rank_grp = "rest of top 10" if inrange(_n,3,10) & !inl
> ist(rank_grp,"china", "uk")
 56.         qui replace rank_grp = "remaining" if mi(rank_grp)
 57.         keep `loc' rank_grp
 58.         qui save ../temp/`loc'_rank_`data'_`samp'`suf', replace
 59.         restore
 60.     }
 61. end

