. program output_tables
  1.     syntax, data(str) samp(str)
  2.     foreach file in {
  3.         qui matrix_to_txt, saving("../output/tables/`file'_`data'_`samp'.t
> xt") matrix(`file'_`data'_`samp') ///
>            title(<tab:`file'_`data'>) format(%20.4f) replace
  4.          }
  5.  end

