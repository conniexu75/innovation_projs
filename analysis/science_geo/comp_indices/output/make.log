. program athr_loc
  1.     syntax, data(str) samp(str)  wt_var(str)
  2.     local suf = cond("`wt_var'" == "cite_affl_wt", "_wt", "") 
  3.     use ../external/cleaned_samps/cleaned_last5yrs_`data'_`samp', clear
  4.     cap drop type
  5.     foreach loc in country msa_c_world inst {
  6.         qui gunique pmid //which_athr //if !mi(affiliation)
  7.         replace `loc' = "harvard university" if "`loc'" == "university har
> vard"
  8.         replace `loc' = "stanford university" if "`loc'" == "university st
> anford"
  9.         local articles = r(unique)
 10.         qui sum `wt_var'
 11.         local total = round(r(sum))
 12.         assert `total' == `articles'
 13.         qui sum `wt_var' if !mi(`loc') 
 14.         local denom = r(sum) 
 15.         preserve
 16.         if inlist("`loc'", "us_state", "area", "msatitle") {
 17.             qui keep if country == "United States"
 18.         }
 19.         gcollapse (sum) `wt_var', by(`loc')
 20.         qui hashsort -`wt_var' 
 21.         qui gen perc = `wt_var' / `total' * 100
 22.         li if mi(`loc')
 23.         qui drop if mi(`loc')
 24.         qui gen cum_perc = sum(perc) 
 25.         qui count
 26.         local rank_end = min(r(N),20) 
 27.         li `loc' perc in 1/`rank_end'
 28.         di "Total articles: `total'"
 29.         mkmat `wt_var' perc cum_perc in 1/`rank_end', mat(top_`loc'_`samp'
> `suf')
 30.         mat top_`loc'_`data'_`samp' = nullmat(top_`loc'_`data'_`samp') , (
> top_`loc'_`samp'`suf')
 31.         qui levelsof `loc' in 1/2
 32.         global top2_`loc'_`data'_`samp' "`r(levels)'"
 33.         if inlist("`loc'", "inst", "city_full", "msatitle", "msa_world", "
> msa_c_world") {
 34.             qui levelsof `loc' in 1/`rank_end'
 35.             global `loc'_`data'_`samp' "`r(levels)'"
 36.         }
 37.         qui gen rank_grp = "first" if _n == 1
 38.         qui levelsof `loc' if _n == 1
 39.         global `loc'_first_`data'_`samp' "`r(levels)'"
 40.         qui levelsof `loc' if _n == 2
 41.         global `loc'_2_`data'_`samp' "`r(levels)'"
 42.         qui replace rank_grp = "second" if _n == 2
 43.         qui replace rank_grp = "rest of top 10" if inrange(_n,3,10)
 44.         qui replace rank_grp = "remaining" if mi(rank_grp)
 45.         keep `loc' perc rank_grp
 46.         qui save ../temp/`loc'_rank_`data'_`samp'`suf', replace
 47.         restore
 48.     }
 49. end

