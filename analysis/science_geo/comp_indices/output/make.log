. program athr_loc
  1.     syntax, data(str) samp(str)  wt_var(str)
  2.     local suf = cond("`wt_var'" == "cite_affl_wt", "_wt", "") 
  3.     use ../external/cleaned_samps/cleaned_last5yrs_`data'_`samp', clear
  4.     gen msa_world = substr(msatitle, 1, strpos(msatitle, ",")) + " US"
  5.     replace msa_world = city_full if country != "United States"
  6. 
.     gen msa_c_world = substr(msa_comb, 1, strpos(msa_comb, ",")) + " US"
  7.     replace msa_c_world = city_full if country != "United States"
  8.     foreach loc in country msa_c_world inst {
  9.         qui gunique pmid //which_athr //if !mi(affiliation)
 10.         replace `loc' = "harvard university" if "`loc'" == "university har
> vard"
 11.         replace `loc' = "stanford university" if "`loc'" == "university st
> anford"
 12.         local articles = r(unique)
 13.         qui sum `wt_var'
 14.         local total = round(r(sum))
 15.         assert `total' == `articles'
 16.         qui sum `wt_var' if !mi(`loc') 
 17.         local denom = r(sum) 
 18.         preserve
 19.         if inlist("`loc'", "us_state", "area", "msatitle") {
 20.             qui keep if country == "United States"
 21.         }
 22.         gcollapse (sum) `wt_var', by(`loc')
 23.         qui hashsort -`wt_var' 
 24.         qui gen perc = `wt_var' / `total' * 100
 25.         li if mi(`loc')
 26.         qui drop if mi(`loc')
 27.         qui gen cum_perc = sum(perc) 
 28.         qui count
 29.         local rank_end = min(r(N),20) 
 30.         li `loc' perc in 1/`rank_end'
 31.         di "Total articles: `total'"
 32.         mkmat `wt_var' perc cum_perc in 1/`rank_end', mat(top_`loc'_`samp'
> `suf')
 33.         mat top_`loc'_`data'_`samp' = nullmat(top_`loc'_`data'_`samp') , (
> top_`loc'_`samp'`suf')
 34.         qui levelsof `loc' in 1/2
 35.         global top2_`loc'_`data'_`samp' "`r(levels)'"
 36.         if inlist("`loc'", "inst", "city_full", "msatitle", "msa_world", "
> msa_c_world") {
 37.             qui levelsof `loc' in 1/`rank_end'
 38.             global `loc'_`data'_`samp' "`r(levels)'"
 39.         }
 40.         qui gen rank_grp = "first" if _n == 1
 41.         qui levelsof `loc' if _n == 1
 42.         global `loc'_first_`data'_`samp' "`r(levels)'"
 43.         qui levelsof `loc' if _n == 2
 44.         global `loc'_2_`data'_`samp' "`r(levels)'"
 45.         qui replace rank_grp = "second" if _n == 2
 46.         qui replace rank_grp = "rest of top 10" if inrange(_n,3,10)
 47.         qui replace rank_grp = "remaining" if mi(rank_grp)
 48.         keep `loc' perc rank_grp
 49.         qui save ../temp/`loc'_rank_`data'_`samp'`suf', replace
 50.         restore
 51.     }
 52. end

