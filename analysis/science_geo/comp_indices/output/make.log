. program athr_loc
  1.     syntax, data(str) samp(str)  wt_var(str)
  2.     local suf = cond("`wt_var'" == "cite_affl_wt", "_wt", "") 
  3.     use ../external/cleaned_samps/cleaned_last5yrs_`data'_`samp', clear
  4.     foreach loc in country msa_c_world inst {
  5.         qui gunique pmid //which_athr //if !mi(affiliation)
  6.         replace `loc' = "harvard university" if "`loc'" == "university har
> vard"
  7.         replace `loc' = "stanford university" if "`loc'" == "university st
> anford"
  8.         local articles = r(unique)
  9.         qui sum `wt_var'
 10.         local total = round(r(sum))
 11.         assert `total' == `articles'
 12.         qui sum `wt_var' if !mi(`loc') 
 13.         local denom = r(sum) 
 14.         preserve
 15.         if inlist("`loc'", "us_state", "area", "msatitle") {
 16.             qui keep if country == "United States"
 17.         }
 18.         gcollapse (sum) `wt_var', by(`loc')
 19.         qui hashsort -`wt_var' 
 20.         qui gen perc = `wt_var' / `total' * 100
 21.         li if mi(`loc')
 22.         qui drop if mi(`loc')
 23.         qui gen cum_perc = sum(perc) 
 24.         qui count
 25.         local rank_end = min(r(N),20) 
 26.         li `loc' perc in 1/`rank_end'
 27.         di "Total articles: `total'"
 28.         mkmat `wt_var' perc cum_perc in 1/`rank_end', mat(top_`loc'_`samp'
> `suf')
 29.         mat top_`loc'_`data'_`samp' = nullmat(top_`loc'_`data'_`samp') , (
> top_`loc'_`samp'`suf')
 30.         qui levelsof `loc' in 1/2
 31.         global top2_`loc'_`data'_`samp' "`r(levels)'"
 32.         if inlist("`loc'", "inst", "city_full", "msatitle", "msa_world", "
> msa_c_world") {
 33.             qui levelsof `loc' in 1/`rank_end'
 34.             global `loc'_`data'_`samp' "`r(levels)'"
 35.         }
 36.         qui gen rank_grp = "first" if _n == 1
 37.         qui levelsof `loc' if _n == 1
 38.         global `loc'_first_`data'_`samp' "`r(levels)'"
 39.         qui levelsof `loc' if _n == 2
 40.         global `loc'_2_`data'_`samp' "`r(levels)'"
 41.         qui replace rank_grp = "second" if _n == 2
 42.         qui replace rank_grp = "rest of top 10" if inrange(_n,3,10)
 43.         qui replace rank_grp = "remaining" if mi(rank_grp)
 44.         keep `loc' perc rank_grp
 45.         qui save ../temp/`loc'_rank_`data'_`samp'`suf', replace
 46.         restore
 47.     }
 48. end

