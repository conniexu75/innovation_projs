. program corr_wt 
  1.     syntax, samp(str) 
  2.     foreach loc in country city_full inst {
  3.         use ../temp/`loc'_rank_fund_`samp',clear
  4.         gen cat = "unwt"
  5.         append using ../temp/`loc'_rank_fund_`samp'_wt
  6.         replace cat = "wt" if mi(cat)
  7.         drop rank_grp
  8.         reshape wide perc, i(`loc') j(cat) string
  9.         qui corr percunwt percwt
 10.         local corr = r(rho)
 11.         mat corr_wt_`samp' = nullmat(corr_wt_`samp') , `corr'
 12.     }
 13.     mat corr_wt = nullmat(corr_wt) \ corr_wt_`samp'
 14. end

