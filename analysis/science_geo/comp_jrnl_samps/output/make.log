. program athr_loc
  1.     syntax, data(str) samp(str)  wt_var(str)
  2.     local suf = cond("`wt_var'" == "cite_affl_wt", "_wt", "") 
  3.     use ../external/openalex/cleaned_all_`data'_`samp', clear
  4.     drop if journal_abbr == "PLoS One"
  5.     foreach loc in country msa_c_world inst { 
  6.         qui gunique pmid //which_athr //if !mi(affiliation)
  7.         local articles = r(unique)
  8.         qui sum `wt_var'
  9.         local total = round(r(sum))
 10.         assert `total' == `articles'
 11.         qui sum `wt_var' if !mi(`loc') 
 12.         local denom = r(sum) 
 13.         preserve
 14.         if inlist("`loc'", "us_state", "area", "msatitle") {
 15.             qui keep if country == "United States"
 16.         }
 17.         collapse (sum) `wt_var', by(`loc')
 18.         qui hashsort -`wt_var' 
 19.         qui gen perc = `wt_var' / `total' * 100
 20.         li if mi(`loc')
 21.         qui drop if mi(`loc')
 22.         qui gen cum_perc = sum(perc) 
 23.         qui count
 24.         local rank_end = min(r(N),20) 
 25.         li `loc' perc in 1/`rank_end'
 26.         di "Total articles: `total'"
 27.         mkmat `wt_var' perc cum_perc in 1/`rank_end', mat(top_`loc'_`samp'
> `suf')
 28.         mat top_`loc'_`data'_`samp' = nullmat(top_`loc'_`data'_`samp') , (
> top_`loc'_`samp'`suf')
 29.         qui levelsof `loc' in 1/2
 30.         global top2_`loc'_`data'_`samp' "`r(levels)'"
 31.         if inlist("`loc'", "country", "inst", "city_full", "msatitle", "ms
> a_world", "msa_c_world") {
 32.             qui levelsof `loc' in 1/`rank_end'
 33.             global `loc'_`data'_`samp' "`r(levels)'"
 34.         }
 35.         qui gen rank_grp = "first" if _n == 1
 36.         qui levelsof `loc' if _n == 1
 37.         global `loc'_first "`r(levels)'"
 38.         qui levelsof `loc' if _n == 2
 39.         global `loc'_second "`r(levels)'"
 40.         gen rank = _n 
 41.         keep `loc'  rank perc
 42.         rename perc share
 43.         qui save ../temp/`loc'_rank_`data'_`samp'`suf', replace
 44.         restore
 45.     }
 46. end

