. program main
  1.     di "OUTPUT START"
  2.     foreach athr_type in year_firstlast year {
  3.         di "ALL: impact_cite_affl_wt"
  4.         athr_loc, athr(`athr_type') samp(all) wt_var(impact_cite_affl_wt) 
  5.         top_insts, athr(`athr_type') samp(all) wt_var(impact_cite_affl_wt)
>  
  6.         di "CNS: impact_cite_affl_wt"
  7.         athr_loc, athr(`athr_type') samp(cns) wt_var(impact_cite_affl_wt) 
  8.         output_tables, athr(`athr_type') samp(cns)
  9.         output_tables, athr(`athr_type') samp(all) 
 10.         map, athr(`athr_type') samp(all) 
 11.     }
 12. end

