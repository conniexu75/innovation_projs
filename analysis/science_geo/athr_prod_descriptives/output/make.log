. program main
  1.     di "OUTPUT START"
  2.     foreach athr_type in first_last all {
  3.     if "`athr_type'" == "first_last" local fol "fl"
  4.     if "`athr_type'" == "all" local fol "all"
  5.         foreach var in impact_cite_affl_wt {
  6.             di "CNS: `var'"
  7.             athr_loc, data(newfund) samp(cns) wt_var(`var') fol(`fol')
  8.             di "ALL: `var'"
  9.             athr_loc, data(all) samp(15jrnls) wt_var(`var') fol(`fol')
 10.         }
 11.         qui output_tables, data(newfund) samp(cns) fol(`fol')
 12.         qui output_tables, data(all) samp(jrnls) fol(`fol')
 13.     }
 14. end

. 
. program athr_loc
  1.     syntax, data(str) samp(str)  wt_var(str) fol(str)
  2.     local athr = cond("`fol'" == "fl", "", "_all")
  3.     local suf = ""
  4.     if "`wt_var'" == "cite_affl_wt" local suf "_wt"
  5.     if "`wt_var'" == "impact_cite_affl_wt" local suf "_if_wt"
  6.     if "`wt_var'" == "pat_adj_wt" local suf "_pat"
  7.     if "`wt_var'" == "frnt_adj_wt" local suf "_frnt"
  8.     if "`wt_var'" == "body_adj_wt" local suf "_body"
  9.     if "`data'" == "all" {
 10.         use ../external/`fol'/cleaned_last5yrs_`samp', clear 
 11.     }
 12.     else if "`data'" != "all" {
 13.         use ../external/`fol'/cleaned_last5yrs_`data'_`samp', clear 
 14.     }
 15.     replace inst = "Mass General Brigham" if inlist(inst, "Massachusetts G
> eneral Hospital" , "Brigham and Women's Hospital")
 16.     local end 20
 17.     replace `wt_var' = log(`wt_var')
 18.     foreach loc in country msa_c_world inst msa_comb us_state {
 19.         if "`loc'" == "inst" & ("`wt_var'" != "pat_adj_wt" & "`wt_var'" !=
>  "body_adj_wt") {
 20.             local end 50
 21.         }
 22.         preserve
 23.         if inlist("`loc'", "us_state", "area", "msatitle", "msa_comb") {
 24.             qui keep if country == "United States"
 25.         }
 26.         gen num_pprs = 1 
 27.         bys `loc' year: egen tot_pprs = total(num_pprs)
 28.         collapse (sum) `wt_var' num_pprs (mean) tot_pprs , by(athr_id `loc
> ' year)
 29.         collapse (mean) `wt_var' [fw = num_pprs], by(`loc')
 30.         tw hist `wt_var' , frac bin(50) color(ebblue%50) ytitle("Share of 
> ${`loc'_name}") xtitle("Log Avg. Productivity")
 31.         graph export ../output/figures/dist_`loc'.pdf, replace
 32.         qui hashsort -`wt_var' 
 33.         li if mi(`loc')
 34.         qui drop if mi(`loc')
 35.         gen rank = _n 
 36.         save ../temp/rankings_`loc'`suf'`athr', replace
 37.         drop rank
 38.         qui count
 39.         local rank_end = min(r(N),`end') 
 40.         li `loc' `wt_var' in 1/`rank_end'
 41.         di "Total articles: `total'"
 42.         qui save ../temp/`loc'_rank_`data'_`samp'`suf'`athr', replace
 43.         restore
 44.     }
 45. end

