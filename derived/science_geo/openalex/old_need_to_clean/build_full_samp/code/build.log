. program clean_panel
  1.     syntax, time(str)
  2.     use ${temp}/appended_pprs, clear
  3.     gen year = yofd(dofq(qrtr))
  4.     drop if mi(`time')
  5.     if "`time'" == "year" {
  6.         gcollapse (sum) num_times, by(athr_id inst_id `time')
  7.     }
  8.     fmerge m:1 inst_id using ${temp}/all_inst_chars, assert(2 3) keep(3) n
> ogen
  9.     replace inst_id = new_inst_id 
 10.     replace inst = new_inst 
 11.     gen broad_affl = inst == "Broad Institute"
 12.     gen hhmi_affl = inst == "Howard Hughes Medical Institute"
 13.     gen funder = (strpos(inst, "Trust")> 0 | strpos(inst, "Foundation")>0 
> | strpos(inst, "Fund")>0) & !inlist(type, "education", "facility", "healthcar
> e")
 14.     gduplicates tag athr_id inst_id `time', gen(dup_entry)
 15.     bys athr_id inst_id `time': gegen tot_times = sum(num_times) 
 16.     replace num_times = tot_times
 17.     drop if dup_entry > 0 & mi(new_inst)
 18.     bys athr_id `time': gen num_affls = _N 
 19.     drop if num_affls > 1 & (funder == 1 | broad_affl == 1 | hhmi_affl == 
> 1)
 20.     drop if inst == "Howard Hughes Medical Institute"
 21.     drop if inst_id == "I1344073410"
 22.     drop new_inst new_inst_id dup_entry tot_times type
 23.     gsort athr_id inst_id `time' country city
 24.     gduplicates drop athr_id inst_id `time', force
 25. 
.     // keep inst with the largest num_times in a  year
.     cap drop N
 26.     bys athr_id `time' : gen N = _N 
 27.     bys athr_id `time': gegen max_num_times = max(num_times)
 28.     drop if num_times != max_num_times & N <= 10
 29.     drop max_num_times
 30.     gunique athr_id `time' 
 31.     local N = r(unique)
 32.     // imputation process
.     foreach loc in inst_id inst_id inst_id inst_id inst_id {
 33.         cap drop has_mult 
 34.         cap drop same_as_after 
 35.         cap drop same_as_before 
 36.         cap drop has_before has_after 
 37.         cap drop N
 38.         bys athr_id `time' : gen N = _N 
 39.         if "`time'" == "year" local range 5 
 40.         if "`time'" == "qrtr" local range 20 
 41.         // if there are mult in a  but sandwiched by the same, then choose
>  that one 
.         bys athr_id `time': gen has_mult = _N > 1
 42.         bys athr_id `loc' (`time'): gen same_as_after = `time'[_n+1]-`time
> ' <= `range' & has_mult[_n+1]==0
 43.         by athr_id `loc' (`time'): gen same_as_before = `time' - `time'[_n
> -1] <= `range'  & has_mult[_n-1]==0
 44.         gen sandwiched = has_mult == 1 & same_as_after == 1 & same_as_befo
> re == 1
 45.         bys athr_id `time': gegen has_sandwich = max(sandwiched)
 46.         drop if has_sandwich ==1 & sandwiched == 0 & N<=10 
 47.         drop sandwiched has_sandwich
 48.         bys athr_id `time': replace has_mult = _N > 1
 49.        
.         // now we prioritize the  before
.         bys athr_id `loc' (`time'): replace same_as_after = `time'[_n+1]-`tim
> e' <= `range' & has_mult[_n+1]==0
 50.         by athr_id `loc' (`time'): replace same_as_before = `time' - `time
> '[_n-1] <= `range'  & has_mult[_n-1]==0
 51.         bys athr_id `time': gegen has_before = max(same_as_before)
 52.         drop if has_mult == 1 & has_before == 1 & same_as_before == 0  & N
> <=10
 53.         bys athr_id `time': replace has_mult = _N > 1
 54.         // now do same for the `time' after 
.         bys athr_id `loc' (`time'): replace same_as_after = `time'[_n+1]-`tim
> e' <= `range' & has_mult[_n+1]==0
 55.         by athr_id `loc' (`time'): replace same_as_before = `time' - `time
> '[_n-1] <= `range'  & has_mult[_n-1]==0
 56.         bys athr_id `time': gegen has_after = max(same_as_after)
 57.         drop if has_mult == 1 & has_after == 1 & same_as_after == 0  & N <
> = 10
 58.         bys athr_id `time': replace has_mult = _N > 1
 59.     }
 60.     cap drop N
 61.     bys athr_id `time' : gen N = _N 
 62.     // if there are sandwiched insts no mater what the `time' gap is
.     hashsort athr_id `time'
 63.     gen prev_inst = inst_id[_n-1]
 64.     gen post_inst = inst_id[_n+1]
 65. 
.     gen sandwich = prev_inst == post_inst & prev_inst != inst_id if athr_id[_
> n-1] == athr_id[_n+1] & athr[_n-1] == athr_id 
 66.     replace inst_id = prev_inst if sandwich == 1 & sandwich[_n-1] != 1  & 
> N <=10 
 67.     replace inst = inst[_n-1] if sandwich == 1 & sandwich[_n-1] != 1  & N 
> <=10 
 68.     drop sandwich prev_inst post_inst
 69. 
.     hashsort athr_id `time'
 70.     gen prev_inst = inst_id[_n-1]
 71.     gen post_inst = inst_id[_n+1]
 72. 
.     gen sandwich = prev_inst == post_inst & prev_inst != inst_id if athr_id[_
> n-1] == athr_id[_n+1] & athr[_n-1] == athr_id 
 73.     replace inst_id = prev_inst if sandwich == 1 & sandwich[_n-1] != 1  & 
> N<=10
 74.     replace inst = inst[_n-1] if sandwich == 1 & sandwich[_n-1] != 1 & N<=
> 10
 75.     drop sandwich
 76.     
.     bys athr_id: gegen modal_inst = mode(inst_id)
 77.     by athr_id `time': replace has_mult = _N > 1
 78.     gen mode_match = inst_id == modal_inst
 79.     bys athr_id `time': gegen has_mode_match = max(mode_match)
 80.     gen mode_yr = year if mode_match == 1
 81.     bys athr_id : gegen min_mode_yr = min(mode_yr) 
 82.     by athr_id : egen max_mode_yr = max(mode_yr) 
 83.     drop if has_mult == 1 & has_mode_match == 1 & mode_match == 0 & inrang
> e(year, min_mode_yr, max_mode_yr) & N <= 10
 84. 
.     gen rand = rnormal(0,1)
 85.     gsort athr_id `time' rand
 86.     gduplicates drop athr_id `time', force
 87.     gunique athr_id `time'
 88.     assert r(unique) == `N'
 89.     drop if mi(`time')
 90.     gisid athr_id `time'
 91.     keep athr_id inst_id `time' num_times inst country_code country city r
> egion broad_affl hhmi_affl
 92. 
.     // do some final cleaning
.     cap gen year  = yofd(dofq(qrtr))
 93. *    drop if !inrange(year, 1945, 2023) 
.     save ${temp}/athr_panel, replace
 94.     
.     import delimited using ../external/geo/us_cities_states_counties.csv, cle
> ar varnames(1)
 95.     glevelsof statefull , local(state_names)
 96.     use ${temp}/athr_panel, clear
 97.     foreach s in `state_names' {
 98.         replace region = "`s'" if mi(region) & country_code == "US" & strp
> os(inst, "`s'")>0
 99.     }
100.     replace region = "Pennsylvania" if country_code == "US" & inlist(city,
>  "Pittsburgh" , "Philadelphia", "Radnor", "Swarthmore", "Meadville", "Lancast
> er" , "Wilkes-Barr") 
